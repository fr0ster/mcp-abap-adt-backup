{
  "name": "@mcp-abap-adt/adt-backup",
  "version": "0.1.0",
  "description": "ADT backup CLI for SAP ABAP objects (recursive package backups and restores)",
  "main": "dist/cli/adt-backup.js",
  "types": "dist/cli/adt-backup.d.ts",
  "bin": {
    "adt-backup": "dist/cli/adt-backup.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "abap",
    "sap",
    "adt",
    "backup",
    "cli",
    "mcp"
  ],
  "author": "Oleksii Kyslytsia <oleksij.kyslytsja@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/fr0ster/mcp-abap-adt-backup#readme",
  "bugs": {
    "url": "https://github.com/fr0ster/mcp-abap-adt-backup/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fr0ster/mcp-abap-adt-backup.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "lint": "npx biome check --write src",
    "lint:check": "npx biome check src",
    "format": "npx biome format --write src",
    "build": "npm run --silent clean && npx biome check src --diagnostic-level=error && npx tsc -p tsconfig.json",
    "build:fast": "npx tsc -p tsconfig.json"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@mcp-abap-adt/adt-clients": "^0.2.10",
    "@mcp-abap-adt/auth-broker": "^0.2.14",
    "@mcp-abap-adt/auth-providers": "^0.2.10",
    "@mcp-abap-adt/auth-stores": "^0.2.10",
    "@mcp-abap-adt/connection": "^0.2.8",
    "fast-xml-parser": "^5.3.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "^24.2.1",
    "typescript": "^5.9.2"
  }
}
